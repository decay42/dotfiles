[global_config]
  always_split_with_profile = True
  dbus = False
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = One dark
      type = Terminal
    [[[window0]]]
      parent = ""
      size = 900, 600
      type = Window
[plugins]
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    use_system_font = True
  [[One dark]]
    background_color = "#282c34"
    copy_on_selection = True
    cursor_color = "#9da5b4"
    font = Droid Sans Mono for Powerline 10
    foreground_color = "#9da5b4"
    palette = "#21252b:#e06c75:#98c379:#d19a66:#61afef:#c678dd:#56b6c2:#abb2bf:#5c6370:#e06c75:#98c379:#d19a66:#61afef:#c678dd:#56b6c2:#fffefe"
    show_titlebar = False
